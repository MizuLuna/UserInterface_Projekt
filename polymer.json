{
  "shell": "router.js",
  "entrypoint": "index.html",
  "fragments": [ "src/components/studyguide-breadcrumb.js",
      "src/components/studyguide-modul.js",
      "home.js"
  ],
  "npm": true,
  "moduleResolution": "node",
  "sources": ["router.js", "manifest/**", "manifest.json", "package.json"],
  "extraDependencies": [
    "robots.txt",
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "builds": [{
    "bundle": true,
    "js": { 
      "minify": false,
      "compile": "es5",
      "transformModulesToAmd": true
    },
    "addServiceWorker": true,
    "addPushManifest": true
  }]
}
